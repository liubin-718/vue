{
  "name": "vue-study",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --config ./build/webpack.prod.conf.js --progress --mode production",
    "watch": "webpack --config ./build/webpack.prod.conf.js --progress --mode production --watch",
    "build:fat1": "webpack --config ./build/webpack.fat1Test.conf.js --progress --mode production",
    "build:fat2": "webpack --config ./build/webpack.fat2Test.conf.js --progress --mode production",
    "build:dev": "webpack --config ./build/webpack.devTest.conf.js --progress --mode production",
    "build:ShareComponents": "webpack --config ./build/webpack.lib.conf.js",
    "lint-staged": "lint-staged",
    "lint": "npm run lint:js-vue",
    "lint-staged:js-vue": "eslint --ext .js,.vue",
    "lint:fix": "eslint --fix --cache --ext .js,.vue ./src",
    "lint:js-vue": "eslint --cache --ext .js,.vue ./src",
    "dev": "node ./build/dev-server",
    "test": "jest",
    "serve": "vue-cli-service serve"
  },
  "dependencies": {
    "async-validator": "^1.11.2",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.5",
    "es6-promise": "^4.2.4",
    "glob": "^7.1.6",
    "mock": "^0.1.1",
    "promise": "^8.1.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "express": "^4.15.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.0.8",
    "http-proxy-middleware": "^0.19.1",
    "mini-css-extract-plugin": "^0.7.0",
    "mockjs": "^1.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-merge": "^4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "keywords": [
    "vue",
    "webpack"
  ],
  "jest": {},
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
